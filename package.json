{
  "name": "@lorkanss/nestjs-redoc",
  "version": "1.9.8",
  "description": "NestJS ReDoc frontend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorkanss/nestjs-redoc.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Alfonso Reyes",
  "main": "dist/index.js",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc -p ./tsconfig.build.json",
    "format": "prettier --write \"./src/**/*.ts\"",
    "format:check": "prettier --check \"./src/**/*.ts\"",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish": "npm run build",
    "publish": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "sample": "ts-node ./sample/main"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.3.3 || ^10.3.9",
    "@nestjs/core": "^10.3.3",
    "reflect-metadata": "^0.1.14"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/swagger": "^7.3.0",
    "express-basic-auth": "^1.2.0",
    "express-handlebars": "5.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.8",
    "@types/express-handlebars": "^3.1.0",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.17",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "commitizen": "^4.2.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^29.7.0",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "supertest": "^6.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/mxarc/nestjs-redoc/issues"
  },
  "homepage": "https://github.com/mxarc/nestjs-redoc#readme"
}
